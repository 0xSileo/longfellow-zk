{
  "name": "Longfellow-zk amd64 dev",
  "build": {
    "dockerfile": "Dockerfile",
    "options": [
      "--platform=linux/amd64"
    ]
  },
  "features": {},
  "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.cmake-tools",
                "ms-vscode.cpptools"
            ],
            "settings": {
                "terminal.integrated.defaultProfile.linux": "zsh",
                "terminal.integrated.profiles.linux": {
                    "zsh": {
                        "path": "/bin/zsh"
                    }
                }
            }
        }
    }
}
